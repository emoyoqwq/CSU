{
  "$id": "https://example.com/address.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "match",
  "description": "match object for GET request",
  "type": "object",
  "properties": {
    "match_id": {
      "description": "the match_id should be an integer of type string",
      "type": "string",
      "pattern": "[0-9]+"
    },
    "user_id": {
      "description": "user_id should be a nickname",
      "type": "string",
      "pattern": "^.{3,30} ?"
    },
    "match_board": {
      "description": "match_board should be a string",
      "type": "string",
      "pattern": "^([rnbqkbnrpPRNBQKBNR 1-9]*){1,8}\/([rnbqkbnrpPRNBQKBNR 1-9]*){1,8}\/([rnbqkbnrpPRNBQKBNR 1-9]*){1,8}\/([rnbqkbnrpPRNBQKBNR 1-9]*){1,8}\/([rnbqkbnrpPRNBQKBNR 1-9]*){1,8}\/([rnbqkbnrpPRNBQKBNR 1-9]*){1,8}\/([rnbqkbnrpPRNBQKBNR 1-9]*){1,8}\/([rnbqkbnrpPRNBQKBNR 1-9]*){1,8} ?"
    }
  },
  "required":["match_id","user_id"],
  "additionalProperties": true
}
